  Arrivel_Draft_FWD            Decimal?                @db.Decimal(5, 2)
  Arrivel_Draft_AFT            Decimal?                @db.Decimal(5, 2)
  Sailing_Draft_FWD            Decimal?                @db.Decimal(5, 2)
  Sailing_Draft_AFT            Decimal?                @db.Decimal(5, 2)
