  Notice_Of_Readiness_Accepted String?                 @db.VarChar(20)
  Dropped_Anchor               DateTime?               @db.DateTime(0)
  Anchor_Up                    DateTime?               @db.DateTime(0)
  POB_Berthing                 DateTime?               @db.DateTime(0)
  First_Line_Ashore            DateTime?               @db.DateTime(0)
  All_Fast_Alongside           DateTime?               @db.DateTime(0)
  Gangway_Placed_Ashore        DateTime?               @db.DateTime(0)
  Cleared_By_Port_Authorities  DateTime?               @db.DateTime(0)
  End_Of_Preparations_On_Board DateTime?               @db.DateTime(0)
  Loading_Commenced            DateTime?               @db.DateTime(0)
  Loading_Complete             DateTime?               @db.DateTime(0)
  Pilot_On_Board_For_Sailing   DateTime?               @db.DateTime(0)
  Ship_Sailed                  DateTime?               @db.DateTime(0)
  ETA_In_The_Next_Port         DateTime?               @db.DateTime(0)
  Email                        String?                 @db.Text
  horas_operacionais           Decimal?                @db.Decimal(10, 2)
  meta_prancha                 Int?
